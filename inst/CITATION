year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)  

bibentry(bibtype = "Manual",
  title = "{GUSbase}: Genotyping Uncertainty with Sequencing data - Base package",
  author = c(person("Timothy P.", "Bilton")),
  year = year,
  note = note,
  url = "https://github.com/tpbilton/GUSbase",
  textVersion=paste0("Bilton, TP (",year,"). GUSbase: Genotyping Uncertainty with Sequencing data - Base package. R package version ",note,". URL:https://github.com/tpbilton/GUSbase"))
